package test240802.object1;

import java.io.FileNotFoundException;
import java.io.FileWriter;
import java.io.IOException;

public class testClient {
	public void output() {
		
		FileWriter fw = null;
		try {
			fw = new FileWriter("test.txt");
			fw.write(97);
			fw.write(65);
			fw.flush();
			
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		
		} catch (IOException e) {			
			e.printStackTrace();
		} finally {
			try {
				fw.close();
				
			}catch(Exception e) {
				e.printStackTrace();
			}
		}
	}
	
	
}
